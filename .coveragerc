[run]
source = fipe
omit = tests/*

[report]
show_missing = True
omit = *tests/*
exclude_lines = 
    pragma: no cover
    return NotImplemented
    raise NotImplementedError
    if __name__ == .__main__.:
    # This needs to be removed after
    # extensively testing the code
    def _
    msg = .*.
    raise TypeError
    raise ValueError
    raise RuntimeError
